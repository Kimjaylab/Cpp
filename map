#include<iostream>
#include<map>
#include<string>
using namespace std;

int main(void)
{
    map<int, string> m;                          // 정수형 키와 문자열 값을 가지는 map 선언
    m.insert(pair<int, string>(40, "me"));       // (40, "me") 삽입
    m.insert(pair<int, string>(35, "yeon"));     // (35, "yeon") 삽입
    m.insert(pair<int, string>(10, "do"));       // (10, "do") 삽입
    m.insert(pair<int, string>(90, "in"));       // (90, "in") 삽입
    m.insert(pair<int, string>(65, "song"));     // (65, "song") 삽입
    m.insert(pair<int, string>(20, "mit"));      // (20, "mit") 삽입
    m.insert(pair<int, string>(50, "su"));       // (50, "su") 삽입

    map<int, string>::iterator iter;             // 반복자 선언

    for (iter = m.begin(); iter != m.end(); iter++) {
        cout << "[" << iter->first << ", " << iter->second << "]" << " ";  // 반복자 사용하여 키-값 출력
    }
    cout << endl;

    for (iter = m.begin(); iter != m.end(); iter++) {
        cout << "[" << (*iter).first << ", " << (*iter).second << "]" << " ";  // 포인터 접근 방식으로 출력
    }

    return 0;
}

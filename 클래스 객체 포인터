#include<iostream>
using namespace std;

class CPoint {
private:
int x;
int y;
public:
void SetXY(int a, int b) {
x = a;
y = b;
}
void Print() {
cout << "(" << x << ", " << y << ")"
<< endl;
}
};

void main(void)
{
int count;

CPoint* pPo;

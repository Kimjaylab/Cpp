#include <iostream>
#include <string>

using namespace std;

template <typename T>

T const& max(T const& a, T const& b)
{
return a < b ? b : a;
}

int main(void)
{
short s1 = 3;
short s2 = 4;
short s3 = max(s1, s2);
cout << s3 << endl;

int a = 3;
int b = 4;
int c = max(a, b);
cout << c << endl;

double d1 = 1.1f;
double d2 = 2.2f;
double d3 = max(d1, d2);
cout << d3 << endl;

string str1 = "I'm ";
string str2 = "who ";
string str3 = max(str1, str2);
cout << str3 << endl;

return 0;
}

#include <iostream>
using namespace std;

class Equal {
public:
int operator( )(int a, int b)
{
return a == b;
}
};

void main(void)
{
Equal cmp;
cout << "equal(5, 5) : " << cmp(5, 5) << endl; //묵시적 호출
cout << "cmp.operator( )(10, 20) : " << cmp.operator( )(10, 20) << endl; //명시적 호출
cout << "Equal( )(10, 10) : " << Equal()(10, 10) << endl; //임시 객체를 통한 호출
}

#include <iostream>

namespace A {
    // A 네임스페이스 내 add 함수 정의
    void add() {
        printf("A의 add( ) 호출!\n");
    }

    // A 네임스페이스 내 minus 함수 정의
    void minus() {
        printf("A의 minus() 호출!\n");
    }
}

using namespace A;  // A 네임스페이스를 전역으로 사용

int main(void)
{
    add();    // A::add() 호출
    minus();  // A::minus() 호출
    return 0;
}
